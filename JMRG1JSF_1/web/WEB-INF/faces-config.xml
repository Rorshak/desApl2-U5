          <navigation-case>
            <from-outcome>Cancel</from-outcome>
            <to-view-id>/users/list-users.xhtml</to-view-id>
        </navigation-case>
        </navigation-rule>
        
        <navigation-rule>
            <from-view-id>/users/list-users.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>Start</from-outcome>
                <to-view-id>/users/form-users.xhtml</to-view-id>
