            <from-outcome>Save</from-outcome>
            <to-view-id>/users/list-users.xhtml</to-view-id>
        </navigation-case>
          <navigation-case>
            <from-outcome>Remove</from-outcome>
            <to-view-id>/users/list-users.xhtml</to-view-id>
